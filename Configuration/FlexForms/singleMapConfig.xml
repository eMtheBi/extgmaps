<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.sheetTitle</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
                    <settings.flexFormGridSize>
                        <TCEforms>
                            <label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.gridSize</label>
							<config>
								<type>input</type>
								<size>40</size>
							</config>
                        </TCEforms>
                    </settings.flexFormGridSize>
                    <settings.header>
                        <TCEforms>
                            <label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.header</label>
							<config>
								<type>input</type>
								<size>40</size>
							</config>
                        </TCEforms>
                    </settings.header>
                    <settings.title>
                        <TCEforms>
                            <label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.title</label>
							<config>
								<type>input</type>
								<size>40</size>
							</config>
                        </TCEforms>
                    </settings.title>
                    <settings.description>
                        <TCEforms>
                            <label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.description</label>
							<config>
								<type>input</type>
								<size>400</size>
							</config>
                        </TCEforms>
                    </settings.description>
                    <settings.image>
                        <TCEforms>
                            <label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.image</label>
							<config type="array">
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>jpg,gif,png</allowed>
								<max_size>2000</max_size>
								<show_thumbs>1</show_thumbs>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
                        </TCEforms>
                    </settings.image>
                    <settings.flexFormMapIcon>
                        <TCEforms>
                            <label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.flexFormMapIcon</label>
							<config type="array">
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>jpg,gif,png</allowed>
								<max_size>2000</max_size>
								<show_thumbs>1</show_thumbs>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
                        </TCEforms>
                    </settings.flexFormMapIcon>
                    <settings.url>
                        <TCEforms>
                            <label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.url</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<size>40</size>
								<wizards type="array">
									<link type="array">
										<type>popup</type>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard</script>
										<params>
											<blindLinkOptions>mail,url,spec,folder,file</blindLinkOptions>
										</params>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
                        </TCEforms>
                    </settings.url>
					<!--<settings.flexFormMap>-->
						<!--<TCEforms>-->
							<!--<label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:tx_extgmaps_domain_model_page.map</label>-->
							<!--<config>-->
							<!--<type>select</type>-->
							<!--<itemsProcFunc>emthebi\Extgmaps\Services\TceMap->displayMap</itemsProcFunc>-->
							<!--</config>-->
						<!--</TCEforms>-->
					<!--</settings.flexFormMap>-->
					<settings.flexFormMapType>
						<TCEforms>
							<label>LLL:EXT:extgmaps/Resources/Private/Language/locallang_db.xlf:singleMapFlexForm.mapType</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">ROADMAP</numIndex>
										<numIndex index="1">ROADMAP</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">TERRAIN</numIndex>
										<numIndex index="1">TERRAIN</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">HYBRID</numIndex>
										<numIndex index="1">HYBRID</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">SATELLITE</numIndex>
										<numIndex index="1">SATELLITE</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.flexFormMapType>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
